<LinearLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout1"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >

    <RelativeLayout
        android:layout_width="596dp"
        android:layout_height="636dp" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/led1on"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onClick"
                    android:text="Led1 On" />

                <Button
                    android:id="@+id/led1off"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led1 Off" />

                <Button
                    android:id="@+id/led1blink"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led1 Blink" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/led2on"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led2 On" />

                <Button
                    android:id="@+id/led2off"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led2 Off" />

                <Button
                    android:id="@+id/led2blink"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led2 Blink" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/led3on"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led3 On" />

                <Button
                    android:id="@+id/led3off"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led3 Off" />

                <Button
                    android:id="@+id/led3blink"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led3 Blink" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/led4on"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led4 On" />

                <Button
                    android:id="@+id/led4off"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led4 Off" />

                <Button
                    android:id="@+id/led4blink"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led4 Blink" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/led5on"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led5 On" />

                <Button
                    android:id="@+id/led5off"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led5 Off" />

                <Button
                    android:id="@+id/led5blink"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led5 Blink" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/led6on"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led6 On" />

                <Button
                    android:id="@+id/led6off"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led6 Off" />

                <Button
                    android:id="@+id/led6blink"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Led6 Blink" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_weight="0.54" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="读卡器(COM2)"
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <Button
                    android:id="@+id/rfButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="读卡片" />

                <TextView
                    android:id="@+id/snTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" >
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="589dp"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="UPS(COM4)"
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <Button
                    android:id="@+id/upsButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="检测UPS" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="市电电压："
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <EditText
                    android:id="@+id/uspVoltage"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:ems="10" >

                    <requestFocus />
                </EditText>

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="市电状态："
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <EditText
                    android:id="@+id/upsStatus"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="门检测开关"
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <Button
                    android:id="@+id/doorButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.59"
                    android:text="读门检测开关" />

                <CheckBox
                    android:id="@+id/checkBox3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.34"
                    android:text="开" />

                <CheckBox
                    android:id="@+id/checkBox4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.34"
                    android:text="关" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.34" >

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="维护开关     "
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <Button
                    android:id="@+id/systemButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.08"
                    android:text="读维护开关" />

                <CheckBox
                    android:id="@+id/checkBox1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="开" />

                <CheckBox
                    android:id="@+id/checkBox2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="关" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="USBKEY检测"
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <Button
                    android:id="@+id/usbkeyButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="检测USBKEY" />

                <TextView
                    android:id="@+id/usbkeySN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium" />
            </LinearLayout>

        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="683dp"
        android:layout_height="543dp" >

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="密码键盘(COM3)"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" >
                </LinearLayout>

                <Button
                    android:id="@+id/openKeypad"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="打开键盘" />

                <Button
                    android:id="@+id/closeKeypad"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="关闭" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/readKeypadButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="读序列号" />

                <EditText
                    android:id="@+id/readKeypadSN"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/writeKeypadButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="写序列号" />

                <EditText
                    android:id="@+id/writeKeypadSN"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="6.04" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <Button
                    android:id="@+id/openTest"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="按键测试" />

                <Button
                    android:id="@+id/closeTest"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="关闭测试" />

            </LinearLayout>

            <RelativeLayout
                android:layout_width="688dp"
                android:layout_height="match_parent" >

                <Button
                    android:id="@+id/key1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="52dp"
                    android:layout_marginTop="25dp"
                    android:text="1" />

                <Button
                    android:id="@+id/key2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/key1"
                    android:layout_marginLeft="50dp"
                    android:layout_toRightOf="@+id/key1"
                    android:text="2" />

                <Button
                    android:id="@+id/key3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/key2"
                    android:layout_alignBottom="@+id/key2"
                    android:layout_marginLeft="40dp"
                    android:layout_toRightOf="@+id/key2"
                    android:text="3" />

                <Button
                    android:id="@+id/keyCancel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/key3"
                    android:layout_alignBottom="@+id/key3"
                    android:layout_marginLeft="37dp"
                    android:layout_toRightOf="@+id/key3"
                    android:text="Cancel" />

                <Button
                    android:id="@+id/key4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/key1"
                    android:layout_alignRight="@+id/key1"
                    android:layout_below="@+id/key1"
                    android:layout_marginTop="30dp"
                    android:text="4" />

                <Button
                    android:id="@+id/key5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/key4"
                    android:layout_alignBottom="@+id/key4"
                    android:layout_alignLeft="@+id/key2"
                    android:text="5" />

                <Button
                    android:id="@+id/key6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/key5"
                    android:layout_alignRight="@+id/key3"
                    android:text="6" />

                <Button
                    android:id="@+id/keyClear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/key6"
                    android:layout_alignLeft="@+id/keyCancel"
                    android:layout_alignRight="@+id/keyCancel"
                    android:text="Clear" />

                <Button
                    android:id="@+id/key8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/key7"
                    android:layout_alignBottom="@+id/key7"
                    android:layout_alignRight="@+id/key5"
                    android:text="8" />

                <Button
                    android:id="@+id/key9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/key8"
                    android:layout_alignBottom="@+id/key8"
                    android:layout_alignLeft="@+id/key6"
                    android:text="9" />

                <Button
                    android:id="@+id/keyEnter"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/key9"
                    android:layout_alignBottom="@+id/key9"
                    android:layout_alignLeft="@+id/keyClear"
                    android:layout_alignRight="@+id/keyClear"
                    android:text="Enter" />

                <Button
                    android:id="@+id/keyDot"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/key7"
                    android:layout_below="@+id/key7"
                    android:layout_marginTop="34dp"
                    android:text="." />

                <Button
                    android:id="@+id/key0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/keyDot"
                    android:layout_alignBottom="@+id/keyDot"
                    android:layout_alignLeft="@+id/key8"
                    android:text="0" />

                <Button
                    android:id="@+id/key00"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/key0"
                    android:layout_alignBottom="@+id/key0"
                    android:layout_alignLeft="@+id/key9"
                    android:text="00" />

                <Button
                    android:id="@+id/keySpace"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/key00"
                    android:layout_alignBottom="@+id/key00"
                    android:layout_alignLeft="@+id/keyEnter"
                    android:layout_alignRight="@+id/keyEnter" />

                <Button
                    android:id="@+id/key7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/key4"
                    android:layout_marginTop="32dp"
                    android:layout_toLeftOf="@+id/key2"
                    android:text="7" />

            </RelativeLayout>

        </LinearLayout>

    </RelativeLayout>

</LinearLayout>